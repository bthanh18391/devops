
PasswordAuthentication no
AllowAgentForwarding yes
AllowTcpForwarding no
X11Forwarding no

Match Group jail
    ChrootDirectory /var/jail

Match Group crews
    AuthorizedKeysCommand /etc/ssh/gate/akc-crew.sh
    AuthorizedKeysCommandUser devops

Match Group sftp
    AllowTcpForwarding yes
    PermitOpen sftp:22
